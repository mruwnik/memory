{
  "pythonVersion": "3.11",
  "include": ["src", "tests", "tools"],
  "exclude": [
    "db/migrations",
    "**/node_modules",
    "**/__pycache__",
    ".venv",
    "venv"
  ],
  "ignore": [
    "db/migrations"
  ],
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "reportGeneralTypeIssues": true,
  "executionEnvironments": [
    {
      "root": "db/migrations",
      "reportMissingImports": false,
      "reportGeneralTypeIssues": false
    }
  ]
}
